    private void MayberryMouseClicked(java.awt.event.MouseEvent evt) {                                      
        try{
            Connection empty = DriverManager.getConnection("jdbc:mysql://localhost:3306/javasa522?zeroDateTimeBehavior=CONVERT_TO_NULL", "root", "Jason1601");
            String ideas = "UPDATE teachers SET Name = ?, Surname = ?, Cell_Number = ?, Address = ?, Gender = ?, Age_group = ?, Email_Address = ? WHERE ID_Number = ?";
            PreparedStatement mint = empty.prepareStatement(ideas);
            mint.setString(1, trap.getText());
            mint.setString(2, thot.getText());
            mint.setString(3, dumptruck.getText());
            mint.setString(4, Loona.getText());
            mint.setString(5, Millie.getText());
            mint.setString(6, Lum.getSelectedItem().toString());
            mint.setString(7, Verosika_Mayday.getText());
            mint.setString(8, femboy.getSelectedItem().toString());           
            mint.executeUpdate();
        }catch(SQLException dont_wanna){
            
        }
    }                                     

