    private void kinkyMouseClicked(java.awt.event.MouseEvent evt) {                                   
        owo(kinky);
        fishnets(steamy);
        fishnets(saucy);
        fishnets(baka);
        imouto.removeAll();
        imouto.add(yuri);
        imouto.repaint();
        imouto.revalidate();
    }                                  

    private void steamyMouseClicked(java.awt.event.MouseEvent evt) {                                    
        owo(steamy);
        fishnets(kinky);
        fishnets(saucy);
        fishnets(baka);
        imouto.removeAll();
        imouto.add(shawty);
        imouto.repaint();
        imouto.revalidate();
    }                                   

    private void saucyMouseClicked(java.awt.event.MouseEvent evt) {                                   
        owo(saucy);
        fishnets(kinky);
        fishnets(steamy);
        fishnets(baka);
        imouto.removeAll();
        imouto.add(Makima);
        imouto.repaint();
        imouto.revalidate();
    }                                  

    private void uwuMouseClicked(java.awt.event.MouseEvent evt) {                                 
        System.exit(0);
    }                                

    private void boobaMouseClicked(java.awt.event.MouseEvent evt) {                                   
        Senpai shorts = new Senpai();
        shorts.setVisible(true);
        this.setVisible(false);
    }                                  

    private void bakaMouseClicked(java.awt.event.MouseEvent evt) {                                  
        owo(baka);
        fishnets(kinky);
        fishnets(saucy);
